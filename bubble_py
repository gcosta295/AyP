
def bubble_sort(vector):
    n=len(vector)
    for i in range(0,n,+1):
        for j in range(i,0,-1):
            print(j)
            if vector[j]<vector[j-1]:
                aux=vector[j]
                vector[j]=vector[j-1]
                vector[j-1]=aux
    return vector

lista=[1,6,7,4,19,3,8,5] #preguntar porque no funciona con todos


print(bubble_sort(lista))